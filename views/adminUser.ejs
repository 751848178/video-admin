<% include header %>

	<table class="ui striped table">
	 	<thead>
	    	<tr>
		      	<th>ID</th>
		      	<th>用户名</th>
	    	</tr>
	  	</thead>
		<tbody>
		 <% users.forEach(function(user){ %>
			<tr>
	    		<td><%=  user.id %></td>
	    		<td><%=  user.username %></td>
	    	</tr>
		 <% }) %>
		</tbody>
	</table>
	<div id="page">
		<% for(var i = 1; i <= dataLength ; i++ ){ %>
			<% if( nowPage == i){ %>
				<a class="active" href="/adminUser?page=<%= i %>">
					<%= i %>
				</a>
				<% }else{ %>
					<a href="/adminUser?page=<%= i %>">
						<%= i %>
					</a>
			<% } %>
		<% } %>
	</div>
<% include footer %>          